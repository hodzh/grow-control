{
  "name": "grow-control",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "start.browser": "ng serve --port 4300 --configuration=browser",
    "build": "ng build",
    "build.prod": "ng build --prod",
    "build.android": "ng build --configuration=android",
    "build.android.prod": "ng build --configuration=android-production",
    "test": "ng test",
    "watch": "ng test --watch",
    "snapshot": "ng test --updateSnapshot",
    "coverage": "ng test --coverage",
    "debug": "node --inspect -r ts-node/register node_modules/jest/bin/jest --runInBand --watch",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "format": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
    "e2e": "ng e2e",
    "mdi-copy": "ts-node ./src/tools/mdi-copy.ts",
    "code-gen": "ts-node --dir=./src/tools code-gen"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.8",
    "@angular/cdk": "11.0.3",
    "@angular/common": "11.0.8",
    "@angular/compiler": "11.0.8",
    "@angular/core": "11.0.8",
    "@angular/forms": "11.0.8",
    "@angular/material": "11.0.3",
    "@angular/platform-browser": "11.0.8",
    "@angular/platform-browser-dynamic": "11.0.8",
    "@angular/pwa": "0.1100.6",
    "@angular/router": "11.0.8",
    "@angular/service-worker": "11.0.8",
    "@ngrx/effects": "10.1.2",
    "@ngrx/store": "10.1.2",
    "bootstrap": "4.5.3",
    "core-js": "3.8.2",
    "echarts": "4.9.0",
    "echarts-gl": "1.1.1",
    "hammerjs": "2.0.8",
    "idb": "5.0.8",
    "ngx-echarts": "4.2.2",
    "rxjs": "6.6.3",
    "smoothie": "1.35.0",
    "tslib": "2.0.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.0.0",
    "@angular-devkit/build-angular": "0.1100.6",
    "@angular-devkit/schematics": "11.0.6",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/schematics": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "11.0.6",
    "@angular/compiler-cli": "11.0.8",
    "@angular/language-service": "11.0.8",
    "@mdi/svg": "5.8.55",
    "@types/dom-serial": "1.0.0",
    "@types/echarts": "4.9.3",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.20",
    "@types/web-bluetooth": "0.0.9",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "codelyzer": "6.0.1",
    "cordova": "10.0.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.0.3",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "glob": "7.1.6",
    "inquirer": "7.3.3",
    "jest": "26.6.3",
    "jest-marbles": "2.5.1",
    "jest-preset-angular": "8.3.2",
    "jest-snapshot": "26.6.2",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "serialport": "9.0.4",
    "svg-sprite": "1.5.0",
    "ts-node": "9.1.1",
    "typescript": "4.0.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "collectCoverageFrom": [
      "src/app/**/*.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cordova/"
    ]
  }
}
